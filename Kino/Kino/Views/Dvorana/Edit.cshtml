@model Kino.Models.Dvorana

@{
    ViewData["Title"] = "Edit";
}

<h2>Uredi</h2>
<br />
<h4>Dvorana</h4>
<br />
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdDvorane" />
            <div class="form-group">
                <label asp-for="IdKolosej" class="control-label"></label>
                <input asp-for="IdKolosej" class="form-control" />
                <span asp-validation-for="IdKolosej" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdTehnologije" class="control-label"></label>
                <input asp-for="IdTehnologije" class="form-control" />
                <span asp-validation-for="IdTehnologije" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ime" class="control-label"></label>
                <input asp-for="Ime" class="form-control" />
                <span asp-validation-for="Ime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="StVrst" class="control-label"></label>
                <input asp-for="StVrst" class="form-control" />
                <span asp-validation-for="StVrst" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="StSedezovNaVrsto" class="control-label"></label>
                <input asp-for="StSedezovNaVrsto" class="form-control" />
                <span asp-validation-for="StSedezovNaVrsto" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Nazaj na seznam</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
